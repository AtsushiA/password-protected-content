{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "telex/password-protected-content",
  "version": "0.1.3",
  "title": "Password Protected Content",
  "category": "common",
  "icon": "lock",
  "description": "Hide content behind a password with client-side encryption",
  "example": {},
  "attributes": {
    "password": {
      "type": "string",
      "default": ""
    },
    "lockedMessage": {
      "type": "string",
      "default": "このコンテンツはパスワードで保護されています。"
    },
    "content": {
      "type": "string",
      "default": ""
    },
    "linkUrl": {
      "type": "string",
      "default": ""
    },
    "linkText": {
      "type": "string",
      "default": "詳細を見る"
    },
    "linkTarget": {
      "type": "string",
      "default": "_self"
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "textdomain": "password-protected-content",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}